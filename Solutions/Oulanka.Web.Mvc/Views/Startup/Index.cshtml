@model Oulanka.Web.Mvc.Models.StartupViewModel

@{
    ViewBag.Title = "StartUp: " + Model.DisplayName;
    Layout = "~/Views/Shared/_LayoutStartUp.cshtml";
}


<div class="row">
    <div class="col-lg-12">
        <h1 class="align-center">Welcome @Model.DisplayName</h1>
        <h3 class="tagline align-center">Start by adding a website to your dashboard:</h3>
    </div>
</div>

@{ Html.RenderAction("new", "websites"); }


@section scripts
{
   @Scripts.Render("~/bundles/dashboard") 
}
